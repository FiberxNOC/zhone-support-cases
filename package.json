{
  "name": "zhone-support-cases",
  "version": "1.0.0",
  "description": "Reporte de casos de soporte device-providers desde Notion, listo para GitHub",
  "type": "module",
  "scripts": {
    "check": "node scripts/check-notion.js",
    "sync": "node scripts/sync-from-notion.js",
    "translate": "node scripts/translate-to-english.js",
    "correct-spelling": "node scripts/correct-spelling.js",
    "build": "node scripts/build-report.js",
    "report": "npm run sync && npm run build",
    "report:all": "npm run sync && npm run translate && npm run build && npm run pdf",
    "pdf": "node scripts/build-pdf-report.js",
    "serve": "node scripts/serve.js",
    "verify": "node scripts/verify.js",
    "test": "node scripts/verify.js --build",
    "check-props": "node scripts/check-props.js"
  },
  "keywords": [
    "notion",
    "support-cases",
    "report"
  ],
  "license": "MIT",
  "dependencies": {
    "@notionhq/client": "^2.2.15",
    "dotenv": "^16.4.5",
    "express": "^4.21.0"
  },
  "devDependencies": {
    "pdf-lib": "^1.17.1",
    "puppeteer": "^23.0.0"
  }
}
